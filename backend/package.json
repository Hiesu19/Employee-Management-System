{
  "name": "backend nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "migrate-seed": "npm run migrate && npm run seed",
    "migrate-seed-prod": "npm run migrate && sequelize-cli db:seed --seed 20250710070051-create-root-user.js",
    "start:test": "cross-env NODE_ENV=test nodemon src/index.js",
    "migrate:test": "cross-env NODE_ENV=test sequelize-cli db:migrate",
    "seed:test": "cross-env NODE_ENV=test sequelize-cli db:seed:all",
    "test:unit": "jest --verbose --testMatch=**/tests/unit/**/*.test.js",
    "test:e2e": "docker compose -f docker-compose.test.yml up --build",
    "start:pm2:test": "cross-env NODE_ENV=test pm2 start src/index.js -i 6 --name backend-test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/client-sqs": "^3.844.0",
    "@aws-sdk/lib-storage": "^3.844.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "nanoid": "^3.3.11",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "supertest": "^7.1.4"
  }
}
